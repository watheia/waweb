{
  "root": "libs/theme/tokens/global",
  "sourceRoot": "libs/theme/tokens/global/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/theme/tokens/global",
        "tsConfig": "libs/theme/tokens/global/tsconfig.lib.json",
        "project": "libs/theme/tokens/global/package.json",
        "entryFile": "libs/theme/tokens/global/src/index.ts",
        "external": ["react/jsx-runtime"],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "libs/theme/tokens/global/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/theme/tokens/global/**/*.{ts,tsx,js,jsx}"]
      }
    }
  }
}
