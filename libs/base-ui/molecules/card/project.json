{
  "root": "libs/base-ui/molecules/card",
  "sourceRoot": "libs/base-ui/molecules/card/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/base-ui/molecules/card",
        "tsConfig": "libs/base-ui/molecules/card/tsconfig.lib.json",
        "project": "libs/base-ui/molecules/card/package.json",
        "entryFile": "libs/base-ui/molecules/card/src/index.ts",
        "external": ["react/jsx-runtime"],
        "rollupConfig": "rollup.config.js",
        "compiler": "babel",
        "assets": [
          {
            "glob": "libs/base-ui/molecules/card/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/base-ui/molecules/card/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/base-ui/molecules/card"],
      "options": {
        "jestConfig": "libs/base-ui/molecules/card/jest.config.js",
        "passWithNoTests": true
      }
    }
  }
}
