{
  "root": "libs/tools/use-modular-css",
  "sourceRoot": "libs/tools/use-modular-css/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/node:package",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/tools/use-modular-css",
        "tsConfig": "libs/tools/use-modular-css/tsconfig.lib.json",
        "packageJson": "libs/tools/use-modular-css/package.json",
        "main": "libs/tools/use-modular-css/src/index.ts",
        "assets": ["libs/tools/use-modular-css/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/tools/use-modular-css/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/tools/use-modular-css"],
      "options": {
        "jestConfig": "libs/tools/use-modular-css/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
